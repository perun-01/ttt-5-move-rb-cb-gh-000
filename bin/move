#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
board = ["   ","   ","   ","   ","   ","   ","   ","   ","   "]
puts "Welcome to Tic Tac Toe!"
puts "Where would you like to go?"
input = gets.strip
index = input_to_index(input)
puts "What value you want to put?"
value = gets.strip
move(board, index, value)
display_board(board)
